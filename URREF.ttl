###  http://eturwg.c4i.gmu.edu/files/ontologies/URREF#appliesTo
:appliesTo rdf:type owl:ObjectProperty ;
           rdfs:domain [ rdf:type owl:Restriction ;
                         owl:onProperty :appliesTo ;
                         owl:someValuesFrom :UncertaintyRepresentation
                       ] ;
